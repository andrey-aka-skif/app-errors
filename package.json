{
  "name": "@andrey-aka-skif/app-errors",
  "version": "2.0.0",
  "description": "Классы ошибок приложения, связанные с обращением к внешнему API и внутренней логикой",
  "keywords": [
    "errors",
    "api",
    "axios"
  ],
  "license": "MIT",
  "author": "@andrey-aka-skif",
  "homepage": "https://github.com/andrey-aka-skif/app-errors/tree/release/v2",
  "bugs": {
    "url": "https://github.com/andrey-aka-skif/app-errors/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/andrey-aka-skif/app-errors.git"
  },
  "type": "module",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/es/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./error-types": {
      "import": "./dist/errorTypes.js",
      "require": "./dist/errorTypes.cjs",
      "default": "./dist/errorTypes.js"
    },
    "./logic": {
      "import": "./dist/logicAppError.js",
      "require": "./dist/logicAppError.cjs",
      "default": "./dist/logicAppError.js"
    },
    "./axios": {
      "import": "./dist/axiosNetworkAppError.js",
      "require": "./dist/axiosNetworkAppError.cjs",
      "default": "./dist/axiosNetworkAppError.js"
    },
    "./superagent": {
      "import": "./dist/superagentNetworkAppError.js",
      "require": "./dist/superagentNetworkAppError.cjs",
      "default": "./dist/superagentNetworkAppError.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.{js,jsx,json,md,html}\""
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.0.0",
    "jsdom": "^26.0.0",
    "prettier": "^3.7.4",
    "terser": "^5.44.1",
    "vite": "^6.4.1"
  }
}
